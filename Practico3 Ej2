#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;

int cont(int x);
void main()
{
int n,s=0;
cout<<"Digite la cantidad de N numeros a sumar: "<<endl;
cin>>n;
s=cont(n);
cout<<"El resultado de la serie es: "<<s<<endl;
getch();
}
int cont(int x)
{
	int s=0,i;
	for (i=1;i<=x;i=i+2)
	{
		s=s+i;
	}
	return s;
}
