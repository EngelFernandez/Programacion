#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;

int cont(int n);
void main()
{
int n,s=0;
cout<<"Digite la cantidad de N: "<<endl;
cin>>n;
s=cont(n);
cout<<"El resultado de la serie es: "<<s<<endl;
getch();
}
int cont(int n)
{
	float i,s=1;
	for(i=1;i<=n;i++)
	{
	s=s*i;
	}
	if (s==0){
		s=1;}
	return s;
}
